{
  "name": "flex-wallboard",
  "description": "Simple dashboard to show contact center statistics",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "deploy": "twilio-run deploy",
    "start": "twilio-run start",
    "dev": "next dev",
    "next-build:dev": "env-cmd -f .env.development next build && next export -o assets/",
    "next-build:uat": "env-cmd -f .env.uat next build && next export -o assets/",
    "next-build:prod": "env-cmd -f .env.production next build && next export -o assets/",
    "next-build": "next build && next export -o assets/",
    "next-start": "next start",
    "lint": "next lint",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "moment-timezone": "^0.5.37",
    "request": "^2.88.2",
    "twilio": "^3.56",
    "@twilio-labs/serverless-runtime-types": "^2.2.3",
    "@twilio/runtime-handler": "1.3.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.0",
    "@next/eslint-plugin-next": "^12.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@twilio-paste/core": "^15.1.0",
    "@twilio-paste/icons": "^9.0.0",
    "@types/node": "^17.0.33",
    "@types/react-timeago": "^4.1.3",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^7.32.0",
    "moment": "^2.29.4",
    "next": "^12.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "3.4.3",
    "twilio-run": "^3.5.3",
    "twilio-sync": "^0.12.2",
    "typescript": "^5"
  },
  "resolutions": {
    "@types/react": "^17.0.31"
  },
  "engines": {
    "node": "16"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
